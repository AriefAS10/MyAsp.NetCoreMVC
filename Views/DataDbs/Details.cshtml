@model DataPribadiNetCoreMVC.Models.DataDb

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<h3>@Html.DisplayFor(model => model.NamaLengkap)</h3>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NamaLengkap)
        </dt>
        <dd class = "col-sm-10">
            : @Html.DisplayFor(model => model.NamaLengkap)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.JenisKelamin)
        </dt>
        <dd class = "col-sm-10">
            : @Html.DisplayFor(model => model.JenisKelamin)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TanggalLahir)
        </dt>
        <dd class = "col-sm-10">
            : @Html.DisplayFor(model => model.TanggalLahir)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Alamat)
        </dt>
        <dd class = "col-sm-10">
            : @Html.DisplayFor(model => model.Alamat)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Country)
        </dt>
        <dd class = "col-sm-10">
            : @Html.DisplayFor(model => model.Country.Negara)
        </dd>
    </dl>
</div>
<div class="form-group">
    <a class="btn btn-primary" style="width:15%" asp-action="Edit" asp-route-id="@Model?.Nik">Edit</a>
    <a class="btn btn-info" style="width:15%" asp-action="Index">Back</a>
</div>
