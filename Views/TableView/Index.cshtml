@model DataPribadiNetCoreMVC.Models.tableVM

<h2>Data Pribadi Master Data</h2>
<hr />
<!--=======================================Table 1============================================-->
<table id="table_id" class="table table-striped table-bordered">
     <thead>
        <tr>
            <th>
                NIK
            </th>
            <th>
                Nama Lengkap
            </th>
            <th>
                Jenis Kelamin
            </th>
            <th>
                Tanggal Lahir
            </th>
            <th>
                Alamat
            </th>
            <th>
                Negara
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.DataDbs)
        {
                <tr>
                    <td>
                    @Html.DisplayFor(modelItem => item.Nik)
                    </td>
                    <td>
                    @Html.DisplayFor(modelItem => item.NamaLengkap)
                    </td>
                    <td>
                    @Html.DisplayFor(modelItem => item.JenisKelamin)
                    </td>
                    <td>
                    @Html.DisplayFor(modelItem => item.TanggalLahir)
                    </td>
                    <td>
                    @Html.DisplayFor(modelItem => item.Alamat)
                    </td>
                    <td>
                    @Html.DisplayFor(modelItem => item.Country.Negara)
                    </td>
                </tr>
        }
    </tbody>
</table>
<br />
<!--=======================================Table 2============================================-->
<h2>Job Title Master Data</h2>
<hr />
<table id="table_id" class="table table-striped">
    <thead>
        <tr>
            <th>
                Job Code
            </th>
            <th>
                Job Title
            </th>
            <th>
                Department
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.JobTitles)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.JobCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.JobTitle1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Department.DepartmentName)
                </td>
            </tr>
        }
    </tbody>
</table>